<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_iabv"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_iabv"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ExibirPdfActivity"></activity>
        <activity android:name=".activity.Estudos" />
        <activity android:name=".activity.CadastroConsolidadorActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.Cadastro" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>